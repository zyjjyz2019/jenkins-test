    stage('Build') {
        sh 'git branch'
        sh 'git status'
        checkout scm
        echo 'Building dev v2.4'
        sh 'git branch'
        sh 'cat Jenkinsfile'
        sh 'git status'
    }
    stage('Test') {
        echo 'Building dev....'
        sh 'git branch'
        sh 'cat Jenkinsfile'
    }
    stage('Deploy') {
        echo 'Building dev....'
        sh 'git branch'
        sh 'cat Jenkinsfile'
    }
}
