pipeline {
         agent {
                label 'pipeline_demo'
               }
         stages {
                 stage('Build') {
                                steps {
                                    echo 'building your app!'
                                }
                 }
                 stage('Test') {
                                steps {
                                    echo 'testing your app!'
                                }
                 }
                 stage('Deploy') {
                                  steps {
                                        echo 'deploying your app!'
                                  }
                 }
              }
}

